<p id="notice"><%= notice %></p>

<h1>Listing Admin Foundations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Mission</th>
      <th>View</th>
      <th>Phone</th>
      <th>Mail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_foundations.each do |admin_foundation| %>
      <tr>
        <td><%= admin_foundation.name %></td>
        <td><%= admin_foundation.description %></td>
        <td><%= admin_foundation.mission %></td>
        <td><%= admin_foundation.view %></td>
        <td><%= admin_foundation.phone %></td>
        <td><%= admin_foundation.mail %></td>
        <td><%= link_to 'Show', admin_foundation %></td>
        <td><%= link_to 'Edit', edit_admin_foundation_path(admin_foundation) %></td>
        <td><%= link_to 'Destroy', admin_foundation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foundation', new_admin_foundation_path %>
